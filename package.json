{
  "name": "gatsby-starter-styled-components",
  "private": true,
  "description": "A simple starter to get up and developing quickly with Gatsby",
  "version": "0.1.0",
  "author": {
    "name": "Scott Spence",
    "email": "yo@scottspence.dev",
    "url": "https://scottspence.com"
  },
  "main": "gatsby-browser.js",
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "format": "pretty-quick"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "dependencies": {
    "@types/react": "17.0.33",
    "@types/react-dom": "17.0.9",
    "@types/react-helmet": "6.1.3",
    "@types/styled-components": "5.1.15",
    "babel-plugin-styled-components": "1.13.2",
    "gatsby": "3.14.2",
    "gatsby-image": "3.11.0",
    "gatsby-plugin-google-fonts": "1.0.1",
    "gatsby-plugin-manifest": "3.14.0",
    "gatsby-plugin-offline": "4.14.0",
    "gatsby-plugin-react-helmet": "4.14.0",
    "gatsby-plugin-sharp": "3.14.1",
    "gatsby-plugin-styled-components": "4.14.0",
    "gatsby-source-filesystem": "3.14.0",
    "gatsby-transformer-sharp": "3.14.0",
    "prop-types": "15.7.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-helmet": "6.1.0",
    "react-seo-component": "2.0.2",
    "styled-components": "5.3.1",
    "styled-reset": "4.3.4"
  },
  "devDependencies": {
    "gatsby-plugin-ts-config": "^2.1.3",
    "husky": "^7.0.4",
    "prettier": "^2.4.1",
    "pretty-quick": "^3.1.1"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "printWidth": 70
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spences10/gatsby-starter-styled-components.git"
  },
  "bugs": {
    "url": "https://github.com/spences10/gatsby-starter-styled-components/issues"
  },
  "homepage": "https://github.com/spences10/gatsby-starter-styled-components#readme"
